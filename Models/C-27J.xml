  <!-- Propeller Left -->
  <animation>
    <name>HeliceCompleteGauche</name>
    <object-name>SixBladesL</object-name>
    <object-name>BolL</object-name>
    <object-name>propdiscL</object-name>
    <object-name>propblurL</object-name>
  </animation>

  <!-- Traslazione dell'oggetto visibile in funzion degli rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscL</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurL</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>300.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>SixBladesL</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceCompleteGauche</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
